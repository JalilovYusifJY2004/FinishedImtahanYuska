@using FinalExamYusif.Areas.Admin.ViewModels.Service;
@using FinalExamYusif.Areas.Admin.ViewModels.Setting;
@model UpdateSettingVM


<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Basic form elements</h4>
            <p class="card-description">
                Basic form elements
            </p>
            <form class="forms-sample" method="post" >
          
                <div class="form-group">
                    <label asp-for="Key" for="exampleInputName1">Key</label>
                    <input asp-for="Key" class="form-control" id="exampleInputName1" placeholder="Key">
                    <span asp-validation-for="Key"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Value" for="exampleInputName1">Value</label>
                    <input asp-for="Value" type="text" class="form-control" id="exampleInputName1" placeholder="Value">
                    <span asp-validation-for="Value"></span>
                </div>
              

                <button type="submit" class="btn btn-primary me-2">Submit</button>
                <a asp-action="Index" asp-controller="service">Cancel</a>
            </form>
        </div>
    </div>
</div>